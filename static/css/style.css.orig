	display: flex;
	flex-direction: row;
	justify-content:center;
	align-items: center;
	align-content: center;
}

.in_column{
	display: flex;
	flex-direction: column;
	justify-content:center;
	align-items: center;
	align-content: center;
}

.text-left{
	width: 40vw;
	font-family: Raleway;
	font-size: 23px;
	line-height: 26px;
	letter-spacing: 0.5px;
	padding: 20px;
	margin:0px;
}

.img-right{
	height: 550px;
	width: auto;
}

.title_text{
	font-family: KaushanScript;
	font-size: 55px;
	width: 600px;
	height: 83px;
	padding-left: 83px;
	padding-top: 14px;
	margin: 0;
	background-image: url("../img/brush_5.png");
	/* background-color: red; */
	
	background-size: 100% 100%;
