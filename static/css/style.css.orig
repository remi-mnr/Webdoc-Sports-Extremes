}

#first_layer{
	margin-left: -5%;
	margin-top: -5%;
	width: 110%;
	height: auto;
}

#last_layer{
	width: 105%;
}

.grid{
	position: absolute;
	width: 100vw;
	height: 100vh;
	background-image: url("../img/grid2.png");
	z-index: 9999;
}

.gallery4x4{
	display: flex;
	flex-wrap: wrap;
	height: 550px;
	width: 550px;
}

.gallery_item{
	width: 50%;
	height: 50%;
	background-size: cover;
	background-position: center;
	background-repeat: no-repeat;
}

.gallery_item:hover{
	z-index: 999;
}

.highlight{
	font-family: LemonMilk;
	font-size: 20.5px;
	letter-spacing: 0.9px;
}

.right_slider{
	position: absolute;
	z-index: 999;
	width: auto;
	height: 12vh;
	right: 0px;
	top: 45vh;
	transition: 0.15s ease;
}
.right_slider:hover{
	filter: drop-shadow(0px 0px 23px #00000071);
	height: 13vh;
}

.left_slider{
	position: absolute;
	z-index: 999;
	width: auto;
	height: 12vh;
	left: 0px;
	top: 45vh;
	transition: 0.15s ease;
	transform: rotate(180deg);
}
.left_slider:hover{
	filter: drop-shadow(0px 0px 23px #00000071);
	height: 13vh;
}













/* --------- INDEX --------- */
.menu_container{
	display: flex;
	width: 100vw;
	height: 100vh;
}
.menu_item{
	display: flex;
	height: 100%;
	width: auto;
	justify-content: center;
	flex-grow: 1;
	transition: 0.35s ease;
	background-size: cover;
	background-position: center;
	overflow: hidden;
}
.menu_item a{
	margin: auto;
	text-decoration: none;
	color: #fff;
}
.in_item{
	display: flex;
	width: 200px;
	height: auto;
	flex-direction: column;
	justify-content:center;
	align-items: center;
	cursor: pointer;
}
.menu_item:nth-child(1){
	background-color: rgb(110, 226, 43);
	background-image: url('../img/modedevie.jpg');
}
.menu_item:nth-child(2){
	background-color: rgb(226, 43, 89);
	background-image: url('../img/entrainement.jpg');
}
.menu_item:nth-child(3){
	background-color: rgb(43, 174, 226);
	background-image: url('../img/competition.jpg');
}
.menu_item:nth-child(4){
	background-color: rgb(226, 214, 43);
	background-image: url('../img/finance.jpg');
}
.menu_item:hover{
	flex-grow: 2.5;
}
.menu_title{
	font-family: "KaushanScript", cursive;
	font-size: 40px;
}
.menu_logos{
	margin-top: 20px;
	width: 4vw;
	height: auto;
}
.crewcore{
	color: #fff;
	opacity: 0.7;
	position: absolute;
	font-family: "LemonMilk", sans-serif;
	font-size: 10px;
	text-align: center;
	bottom: 25px;
	letter-spacing: 5px;
	text-decoration: none;
	transition: all 0.3s;
}
.crewcore:hover{
	opacity: 1;
}
.logo{
	position: absolute;
	top: 20px;
	text-align: center;
	width: 8vw;
	height: 8vw;
	border-radius: 100%;	
	background: #fff;
}







