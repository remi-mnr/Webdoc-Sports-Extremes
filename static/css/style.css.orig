	filter: hue-rotate(225deg) ;	
	margin-bottom: 10px;	
}

.videobutton{
	position: absolute;
	z-index: 60;
	width: 6vw;
	height: auto;
    margin-left: 6vw;
	margin-top: 81vh;
	cursor: pointer;
	filter: brightness(0) drop-shadow(5px 5px 15px #000000b7);

	right: 13vw;
    bottom: 18vh;
	
	transition: 0.2s ease;
}

.videobutton:hover{
	filter: brightness(1) drop-shadow(5px 5px 15px #000000b7);
	/* filter: drop-shadow(5px 5px 15px #000v000b7); */
}

.img_panorama{
	height: 41vh;
	width: auto;
}

.panorama_text{
	width: 57vw;
	font-family: Raleway;
	font-size: 23px;
	line-height: 26px;
	letter-spacing: 0.5px;
	padding: 20px;
	margin:0px;
}

.title{
	width: 37vh;
	height: auto;
}

.big_title{
	font-family: LemonMilkBold;
	/* font-style: bold; */
