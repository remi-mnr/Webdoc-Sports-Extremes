body{
	background-color: #424242;
	color: white;
	overflow: hidden;
}

/* ------------ ELEMENTS D'EXEMPLE ------------ */
h1{
	margin-top: 65vh;
	margin-left: 8vw;
	opacity: 0.2;
}

#doggo{
	width: 300px;
    height: auto;
    margin-left: 500px;
    margin-top: 300px;
}

#animh2{
	visibility: visible;
    width: 300px;
    height: auto;
    position: absolute;
    top: 288px;
    left: 297px;
	-webkit-animation-duration: 0.5s;
}

#doggy{
	margin-left: 46vw;
    margin-top: 45vh;
}

.plan{
	width: 80%;
	position: absolute;
	top:-30px;
	left: 200px;
	opacity: 0.3333;
}


/* ------------  ------------ */

.page{
	width: 100vw;
	height: 100vh;
	/*hidden à débattre*/
	overflow: hidden;
	display: flex;
	justify-content:center;
	align-items: center;
	align-content: center;
}

.home_button{
	width: 50px;
	position: absolute;
	z-index: 9999;
	margin-top: 90vh;
    margin-left: 92vw;
	opacity: 0.3;
	transition-duration: 0.25s;
}

.home:hover{
	transition: cubic-bezier(0, 0, 0, 1);
	transition-duration: 0.25s;
	opacity: 0.75;
}

.arrow_top{
	width: 5vw;
	position: absolute;
	z-index: 9999;
	top: 2vh;
	left: 45vw;
	opacity: 0.5;
	transition-duration: 0.25s;
}
.arrow_top:hover{
	transition: cubic-bezier(0, 0, 0, 1);
	transition-duration: 0.25s;
	opacity: 0.9;
}
.arrow_down{
	width: 5vw;
	position: absolute;
	z-index: 9999;
	bottom: 2vh;
	left: 45vw;
	opacity: 0.5;
	transform: rotate(-180deg);
	transition-duration: 0.25s;
}
.arrow_down:hover{
	transition: cubic-bezier(0, 0, 0, 1);
	transition-duration: 0.25s;
	opacity: 0.9;
}
.arrow_left{
	width: 5vw;
	position: absolute;
	z-index: 9999;
	top: 45vh;
	left: 3vw;
	opacity: 0.5;
	transform: rotate(-90deg);
	transition-duration: 0.25s;
}
.arrow_left:hover{
	transition: cubic-bezier(0, 0, 0, 1);
	transition-duration: 0.25s;
	opacity: 0.9;
}
.arrow_right{
	width: 5vw;
	position: absolute;
	z-index: 9999;
	top: 45vh;
	right: 3vw;
	opacity: 0.5;
	transform: rotate(90deg);
	transition-duration: 0.25s;
}
.arrow_right:hover{
	transition: cubic-bezier(0, 0, 0, 1);
	transition-duration: 0.25s;
	opacity: 0.9;
}















